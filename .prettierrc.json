{
	"printWidth": 140,
	"singleQuote": true,
	"trailingComma": "all",
	"tabWidth": 4,
	"useTabs": true,
	"jsxSingleQuote": true,
	"singleAttributePerLine": false,
	"semi": false,
	"plugins": ["prettier-plugin-tailwindcss"],
	"overrides": [
		{
			"files": ["*.{yaml,yml}"],
			"options": {
				"singleQuote": false,
				"useTabs": false,
				"tabWidth": 4
			}
		}
	]
}
